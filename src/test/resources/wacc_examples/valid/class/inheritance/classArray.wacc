# an array is declared with classes of the same type

# Output:
# 2
# 2
# 2
# Should print three 2s.

# Program:

begin
  class d extends e {
  }
  class e extends f {
  }
  class f {
    int f() is
          return 2
        end
  }
  f class1 = new d();
  f class2 = new e();
  f class3 = new f();
  f[] classes = [class1, class2, class3];
  int n = 0;
  while n < 3 do
    n = n + 1 ;
    f nextClass = classes[n];
    int y = call nextClass.f();
    println y
  done ;
  println "Should print three 2s."
end
