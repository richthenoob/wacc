# test the functionality of stdlib power

# Output:
# 3125
# 19683
# 1
# 1

begin
  include "stdlib.wacc"
  int res1 = call power(5,5); # 3125
  int res2 = call power(3,9); # 19683
  int res3 = call power(3,0); # 1
  int res4 = call power(0,0); # we shall expect it to be 1
  println res1;
  println res2;
  println res3;
  println res4
end