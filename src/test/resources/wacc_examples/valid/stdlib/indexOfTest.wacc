# returns the first one if there are duplicates

begin
  include "stdlib.wacc"
  int[] arr1 = [3,1,5,7,9,23];
  int[] arr2 = [-5, -8, -12, -4, -2, -7];
  int[] arr3 = [1,2,3,3,5,6,3];
  int[] arr4 = [];

  int idx1 = call indexOf(arr1, 7);  # 3
  int idx2 = call indexOf(arr2, -7); # 5
  int idx3 = call indexOf(arr3, 3);  # 2
  int idx4 = call indexOf(arr4, 5);   # -1
  int idx5 = call indexOf(arr1, 40);  # -1
  int idx6 = call indexOf(arr2, -40);  # -1

  println idx1;
  println idx2;
  println idx3;
  println idx4;
  println idx5;
  println idx6

end